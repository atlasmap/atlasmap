  "scripts": {
    "ng": "ng",
    "prebuild": "rimraf ./dist",
    "prebuild:css": "ts-node ./scripts/inline-css.ts",
    "build:lib": "ng build @atlasmap/atlasmap-data-mapper-lib",
    "build:app": "ng build @atlasmap/atlasmap-data-mapper",
    "e2e": "ng e2e",
    "inspect": "rm -f test-resources/inspected/*; ../mvnw atlasmap:generate-inspections",
    "lint": "ng lint @atlasmap/atlasmap-data-mapper",
    "lint:fix": "ng lint @atlasmap/atlasmap-data-mapper --fix",
    "pretest": "rm -rf coverage junit",
    "pretest:coverage": "yarn pretest",
    "start": "ng serve -c dev --port 3000 --open",
    "start:trace": "ng serve -c trace --port 3000 --open",
    "start:prod": "ng serve -c production --port 3000 --open",
    "test": "ng test --browsers ChromeHeadlessCustom --watch=false @atlasmap/atlasmap-data-mapper",
    "test:debug": "ng test --browsers ChromeDebug @atlasmap/atlasmap-data-mapper",
    "test:coverage": "ng test --browsers ChromeHeadlessCustom --watch=false --code-coverage @atlasmap/atlasmap-data-mapper",
    "compodoc": "compodoc -p src/tsconfig.app.json -d dist/compodoc",
    "version": "echo $npm_package_version"
  },
